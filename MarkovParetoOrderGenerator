#include <iostream>
#include <random>

enum MarketState{
    NEUTRAL,
    BUY_PRESSURE,
    SELL_PRESSURE
};

class OrderGenerator {
    const double transitionMatrix[3][3] = {
        {0.8, 0.1, 0.1}, // Neutral
        {0.15, 0.8, 0.05}, // Buy Pressure
        {0.15, 0.05, 0.8} // Sell Pressure
    };

    
}
